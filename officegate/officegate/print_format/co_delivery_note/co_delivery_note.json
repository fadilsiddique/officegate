 "css": ".print-format {\n    padding:0px !important;\n}\n\n/*.print-format, body {*/\n/*    margin: 0 !important;*/\n/*    padding: 0 !important;*/\n/*}*/\n\n.contact-stamp {\n    display:flex;\n    justify-content:end;\n}\n@media print {\n    @page {\n        margin: 20px !important;\n    }\n\n\n    body {\n        margin: 0 !important;\n        padding: 0 !important;\n        font-size: 12px;\n    }\n\n\n}\n\n.print-format img {\n    max-width: 100% !important;\n}\n\n.logo {\n    display: flex;\n    justify-content: center;\n    height: 100px;\n    margin-top:10px;\n}\n\n.rec {\n    \n    display: flex;\n    flex-direction: column;\n    justify-content: left;\n    align-items: flex-start;\n    line-height:35.5px;\n}\n\n.full-content {\n    margin-left:20px;\n    margin-right:30px;\n}",
